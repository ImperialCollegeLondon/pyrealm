@article{Triantis:2008id,
	title = {Evolutionary species-area curves as revealed by single-island endemics: insights for the inter-provincial species-area relationship},
	volume = {31},
	url = {http://dx.doi.org/10.1111/j.0906-7590.2007.05323.x},
	doi = {10.1111/j.0906-7590.2007.05323.x},
	abstract = {The species2013area relationship (SAR) between different biological provinces is one of the most interesting, but least explored aspects of the well-known species2013area pattern. Following the usage that a biological province is a region whose species have for the most part evolved within it, rather than immigrating from somewhere else, we propose that islands can be considered equivalent to biological provinces for single island endemic species (SIEs). Hence, analyses of the relationships between numbers of SIEs and island area can be used as model systems to explore the form of inter-provincial SARs. We analyzed 13 different data sets derived from 11 sources, using the power (log2013log) model. Eleven of the SIE2013area relationships were statistically significant, explaining high proportions of the variance in SIE numbers (R2 0.5720130.95). The z-values (slopes) of the statistically significant SIE2013area relationships ranged from 0.47 to 1.13, with a mean value of 0.80 (SD±0.24). All the island systems in which SIE represent 
\&gt;50\% of species exhibited z-values for the SARs of native species higher than those deemed typical of archipelagic SARs. The SIE2013area slopes are quite similar to those previously calculated for inter-provincial SARs, indicating that, when speciation becomes the dominant process adding to the species richness of assemblages, high z-values should be anticipated, regardless of the biogeographical scale of the study system.},
	number = {3},
	journal = {Ecography},
	author = {Triantis, Kostas A and Mylonas, Moisis and Whittaker, Robert J.},
	year = {2008},
	note = {tex.date-added: 2012-04-02T11:04:37GMT
tex.date-modified: 2015-07-01T08:53:19GMT
tex.local-url: file://localhost/Users/dorme/References/Library.papers3/Articles/2008/Triantis/Ecography\%202008\%20Triantis.pdf
