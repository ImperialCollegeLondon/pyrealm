@article{kennedy:2019a,
  title = {Implementing {{Plant Hydraulics}} in the {{Community Land Model}}, {{Version}} 5},
  author = {Kennedy, Daniel and Swenson, Sean and Oleson, Keith W. and Lawrence, David M. and Fisher, Rosie and {Lola da Costa}, Antonio Carlos and Gentine, Pierre},
