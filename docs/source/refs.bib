	pages = {101--128},
	file = {Ecol Monographs 2000 Curran.pdf:/Users/dorme/Zotero/storage/WIFUFBXZ/Ecol Monographs 2000 Curran.pdf:application/pdf;Ecol Monographs 2000 Curran.pdf:/Users/dorme/Zotero/storage/CWPCZ8EZ/Ecol Monographs 2000 Curran.pdf:application/pdf},
}

