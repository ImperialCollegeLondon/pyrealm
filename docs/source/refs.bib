@article{Ceballos:2005dx,
	title = {Global mammal conservation: {What} must we manage?},
	volume = {309},
	url = {http://dx.doi.org/10.1126/science.1114015},
	doi = {DOI 10.1126/science.1114015},
	abstract = {We present a global conservation analysis for an entire "flagship" taxon, [and mammals. A combination of rarity, anthropogenic impacts, and political endemism has put about a quarter of terrestrial mammal species, and a larger fraction of their populations, at risk of extinction. A new global database and complementarity analysis for selecting priority areas for conservation shows that similar to 11\% of Earth's land surface should be managed for conservation to preserve at least 10\% of terrestrial mammal geographic ranges. Different approaches, from protection (or establishment) of reserves to countryside biogeographic enhancement of human-dominated landscapes, will be required to approach this minimal goal.},
	journal = {Science (New York, N.Y.)},
